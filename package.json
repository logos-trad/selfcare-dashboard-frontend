{
  "name": "selfcare-dashboard-frontend",
  "version": "1.0.0",
  "homepage": "dashboard",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@fontsource/titillium-web": "^4.5.0",
    "@mui/icons-material": "^5.0.3",
    "@mui/material": "^5.0.3",
    "@pagopa/ts-commons": "^10.2.1",
    "@reduxjs/toolkit": "^1.6.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^26.0.24",
    "@types/lodash": "^4.14.172",
    "@types/node": "^12.20.19",
    "@types/react": "^17.0.18",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.1.8",
    "@types/redux-logger": "^3.0.9",
    "axios": "^0.23.0",
    "fp-ts": "^2.11.5",
    "io-ts": "^2.2.16",
    "lodash": "^4.17.21",
    "node-fetch": "^3.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-dropzone": "^11.4.2",
    "react-md-spinner": "^1.0.0",
    "react-redux": "^7.2.6",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "redux-logger": "^3.0.6",
    "typescript": "^4.3.5",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test -i",
    "test:coverage": "react-scripts test -i --coverage",
    "eject": "react-scripts eject",
    "lint-autofix": "eslint . -c .eslintrc.js --ext .ts,.tsx --fix",
    "lint": "eslint . -c .eslintrc.js --ext .ts,.tsx ",
    "prettify": "prettier --write \"./**/*.{ts,tsx}\"",
    "generate": "npm-run-all generate:*",
    "generate:api-b4f-dashboard": "rimraf src/api/generated/b4f-dashboard & gen-api-models --api-spec openApi/dashboard-swagger20.json --out-dir src/api/generated/b4f-dashboard --no-strict --request-types --response-decoders --client",
    "generate:api-party-process": "rimraf src/api/generated/party-process & gen-api-models --api-spec openApi/party-process-swagger20.json --out-dir src/api/generated/party-process --no-strict --request-types --response-decoders --client"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@pagopa/openapi-codegen-ts": "^10.0.5",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "7.11.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-functional": "^3.7.2",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-sonarjs": "^0.10.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "src/index.tsx",
      "src/reportWebVitals.ts",
      "src/api/generated"
    ]
  }
}
